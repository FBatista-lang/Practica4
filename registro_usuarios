    private void nombre_userKeyTyped(java.awt.event.KeyEvent evt) {                                     

        char c = evt.getKeyChar();
        if ((c < 'a' || c > 'z') && (c < 'A' || c > 'Z') && (c < ' ' || c > ' ')) {
            evt.consume();
        }

    }                                    

    private void apellido_userKeyTyped(java.awt.event.KeyEvent evt) {                                       
        char c = evt.getKeyChar();
        if ((c < 'a' || c > 'z') && (c < 'A' || c > 'Z') && (c < ' ' || c > ' '))
            evt.consume();        // TODO add your handling code here:
    }                                      

    private void correoKeyTyped(java.awt.event.KeyEvent evt) {                                
        char c = evt.getKeyChar();
        if ((c < 'a' || c > 'z') && (c < 'A' || c > 'Z') && (c < ' ' || c > ' ')&&(c< '@'|| c>'@'))
            evt.consume();        // TODO add your handling code here:
    }                               

    private void id_userKeyTyped(java.awt.event.KeyEvent evt) {                                 
        // TODO add your handling code here:
    }                                

    private void telefonoKeyTyped(java.awt.event.KeyEvent evt) {                                  
        char c = evt.getKeyChar();
        if (c < '0' || c > '9')
            evt.consume();// TODO add your handling code here:
    }                                 

